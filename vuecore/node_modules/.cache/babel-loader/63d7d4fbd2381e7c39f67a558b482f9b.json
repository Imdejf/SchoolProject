         name: null,\r\n                slug: null,\r\n                content: null,\r\n            },\r\n        };\r\n    },\r\n    validations: {\r\n        page: {\r\n            name: { required },\r\n            slug: { required },\r\n            content: { required },\r\n        },\r\n    },\r\n    computed: {\r\n        editMode() {\r\n            return this.$route.params[\"op\"] == \"edit\";\r\n        },\r\n    },\r\n    methods: {\r\n        ...mapActions([\"addPage\", \"editPage\"]),\r\n        async handlePage() {\r\n            this.$v.$touch();\r\n\r\n            if (!this.$v.$invalid) {\r\n                if (this.editMode) {\r\n                    await this.editPage(this.page);\r\n                } else {\r\n                    await this.addPage(this.page);\r\n                }\r\n\r\n                this.$router.push(\"/admin/pages\");\r\n            }\r\n        },\r\n    },\r\n    created() {\r\n        if (this.editMode) {\r\n            Object.assign(\r\n                this.page,\r\n                this.$store.getters.pageById(this.$route.params[\"id\"])\r\n            );\r\n        }\r\n    },\r\n};\r\n</script>\r\n"],"sourceRoot":"src/components/admin"}]}